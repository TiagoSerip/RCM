public class TextNoteDto extends PublicationDto {
	
	private String text;

	/**
	 * @param id
	 * @param label
	 * @param ownerUsername
	 */
	public TextNoteDto(String ownerUsername, String label, String text) {
		super(ownerUsername, label);
		this.text=text;
	}

	/**
	 * @return the text
	 */
	public String getText() {
		return text;
	}

}
