package pt.ist.sonet.service.bridge;

import pt.ist.sonet.exception.AgentUsernameDoesNotExistsException;
import pt.ist.sonet.exception.PagAmigoTransferException;
import pt.ist.sonet.service.ProcessPaymentService;

public class LocalApplicationServer implements ApplicationServerBridge {

	@Override
	public void ProcessPayment(String from, String to, int amount, String description) throws 
		AgentUsernameDoesNotExistsException, PagAmigoTransferException {

		ProcessPaymentService service = new ProcessPaymentService(from, to, amount, description);
		service.execute();

	}

}
