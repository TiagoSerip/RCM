package pt.ist.sonet.domain;

import java.io.Serializable;

/**
 * Classe Permission que herda de Permission_Base
 */
public abstract class Permission extends Permission_Base implements Serializable{
	private static final long serialVersionUID = 1L;

	/**
	 * Construtor
	 */
    protected  Permission() {
    }
    
	/**
	 * Metodo abstracto
	 * 
	 * @param Agent whoWantsToSee
	 * @param Agent toCheck
	 */
    public abstract boolean canAcess(Agent whoWantsToSee, Agent toCheck);
    
	/**
	 * Metodo abstracto
	 */
    public abstract String toString();
    
}
